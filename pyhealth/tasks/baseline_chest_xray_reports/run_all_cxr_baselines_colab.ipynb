      "source": [
        "\n",
        "# ===== Step 6) Load metadata and build the working dataframe =====\n",
        "import pandas as pd\n",
        "\n",
        "meta_path = meta_candidates[0]\n",
        "print(\"Using metadata:\", meta_path)\n",
        "meta = pd.read_csv(meta_path)\n",
        "\n",
        "needed = [\"dicom_id\", \"subject_id\", \"study_id\", \"ViewPosition\"]\n",
        "for col in needed:\n",
        "    if col not in meta.columns:\n",
        "        raise KeyError(f\"Metadata file doesn't contain required column: {col}. \"\n",
        "                       f\"Columns present: {list(meta.columns)[:40]} ...\")\n",
        "\n",
        "df = meta[needed].copy()\n",
        "df[\"rad_id\"] = df[\"study_id\"].apply(lambda x: f\"s{int(x)}\").str.lower()  # match report filenames like s123456.txt\n",
        "df[\"report_path\"] = df[\"rad_id\"].map(studyid_to_path)\n",
        "\n",
        "# Keep only rows where we can find the report\n",
        "df = df.dropna(subset=[\"report_path\"]).copy()\n",
        "print(\"Rows with report_path:\", df.shape)\n",
        "\n",
        "# Filter AP only\n",
        "df = df[df[\"ViewPosition\"].astype(str).str.upper().eq(\"AP\")].copy()\n",
        "print(\"AP-only rows:\", df.shape)\n",
        "\n",
        "if MAX_STUDIES is not None and len(df) > MAX_STUDIES:\n",
        "    df = df.sample(MAX_STUDIES, random_state=SEED).copy()\n",
        "    print(\"After MAX_STUDIES sample:\", df.shape)\n",
        "\n",
        "df.head()\n"
      ],
